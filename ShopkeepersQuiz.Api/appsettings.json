{
  "Name": "Shopkeeper's Quiz API",
  "QuestionSettings": {
    "PreloadedQuestionsCount": 5,
    "QuestionTimeSeconds": 10,
    "AnswerTimeSeconds": 5,
    "IncorrectAnswersGenerated": 5
  },
  "ScraperSettings": {
    "RunEvery": "0 4 ? * THU"
  },
  "ConnectionStrings": {
    "ApplicationDatabase": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*"
}
